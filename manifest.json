
{
  "manifest_version": 2,
  "name": "EyeCatcher",
  "version": "1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "EyeCatcher",
    "default_icon": {
      "16": "images/closed-eye.png",
      "24": "images/opened-eye.png",
      "32": "images/opened-eye.png" 
    }
  }
}